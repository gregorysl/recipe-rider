{
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "impliedStrict": true,
      "classes": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "globals": {
    "document": true
  },
  "rules": {
    "global-require": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "linebreak-style": 0,
    "comma-dangle": [
      "error",
      "never"
    ],
    "react/no-unused-prop-types": "warn",
    "no-unused-vars": "warn",
    "no-debugger": 0,
    "no-console": 0,
    "no-param-reassign": ["error", { "props": false }]
  }
}